{
  "name": "vemplate",
  "publisher": "DengYiQuan",
	"displayName": "",
	"description": "create vue file faster",
	"version": "0.0.1",
	"repository": {
		"type" : "git",
		"url" : "https://github.com/yuchengtian/vemplate"
	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
        "onCommand:vue.template"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "vue.template",
            "title": "New Vue File With Template"

		}],
		"menus": {
            "explorer/context": [
				{
				"command": "vue.template",
				"group": "navigation",
				"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.66.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.3"
    }

}
