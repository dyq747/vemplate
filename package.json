{
  "name": "vemplate",
  "publisher": "yuchengtian",
  "displayName": "Vue Template File",
  "description": "create vue single component file with template",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuchengtian/vemplate"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "icon": "vue-2-3.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vemplate"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vemplate",
        "title": "Vemplate.dev"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vemplate",
          "group": "navigation",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5",
    "@vscode/test-electron": "^2.1.3"
  }
}
